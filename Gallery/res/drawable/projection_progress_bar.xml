<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#4cffffff" android:endColor="#4cffffff" android:angle="0.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress" android:drawable="@drawable/projection_seekbar_progress" />
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#5cffffff" android:endColor="#5cffffff" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>