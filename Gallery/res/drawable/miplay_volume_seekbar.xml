<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/miplay_volume_seekbar_corner_radius" />
            <solid android:color="@color/miplay_volume_seekbar_background_color" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/miplay_volume_seekbar_corner_radius" />
                <solid android:color="@color/miplay_volume_seekbar_foreground_color" />
            </shape>
        </clip>
    </item>
</layer-list>