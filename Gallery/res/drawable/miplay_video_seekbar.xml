<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <size android:height="@dimen/px_12" />
            <corners android:radius="@dimen/miplay_video_seekbar_corner_radius" />
            <solid android:color="@color/miplay_video_seekbar_bg" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/miplay_video_seekbar_corner_radius" />
                <solid android:color="@color/miplay_seekbar_progress" />
            </shape>
        </clip>
    </item>
</layer-list>