<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/album_page_item_corner_radius" />
            <solid android:color="@color/image_pressed_color" />
            <stroke android:width="1.0px" android:color="@color/image_stroke_color" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/album_page_item_corner_radius" />
            <stroke android:width="1.0px" android:color="@color/image_stroke_color" />
        </shape>
    </item>
</selector>