<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/search_radius_large" />
            <solid android:color="@color/search_nav_recommend_item_fg_pressed" />
            <stroke android:width="1.0px" android:color="@color/search_nav_recommend_item_stroke" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/search_radius_large" />
            <stroke android:width="1.0px" android:color="@color/search_nav_recommend_item_stroke" />
        </shape>
    </item>
</selector>